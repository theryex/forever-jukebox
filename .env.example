SPOTIFY_CLIENT_ID=your_client_id
SPOTIFY_CLIENT_SECRET=your_client_secret
YOUTUBE_API_KEY=your_api_key
ADMIN_KEY=
WORKER_COUNT=1
ENGINE_REPO=../engine
ENGINE_CONFIG=../engine/calibration.json
ENGINE_PROGRESS=true
ALLOW_USER_UPLOAD=true
ALLOW_USER_YOUTUBE=true
ALLOW_FAVORITES_SYNC=true

# GPU Mode: cpu | cuda | rocm
GPU_MODE=cpu

# Calibration Mode: legacy | upstream
# - legacy: Uses tuned_config.json with PCA calibration matrices
# - upstream: Uses calibration.json with simpler affine calibration
CALIBRATION_MODE=legacy

# Server port
PORT=8000

# Enable analysis progress output (1 = enabled)
FJ_PROGRESS=0
